{
  "name": "algaeh-document-management",
  "version": "2.0.0",
  "description": "Algaeh document management",
  "main": "dist",
  "scripts": {
    "document_server": "run-p run-webpack run-dev",
    "build": "webpack --config webpack.prod.js",
    "run-webpack": "webpack --watch --config webpack.dev.js",
    "dev": "npm run document_server",
    "lint": "eslint src && eslint test",
    "test": "echo \"Error: no test specified\" && exit 1",
    "run-prod": "cross-env  PORT=3006 NODE_ENV=production  pm2 start  --max-memory-restart 850M -i 1 build/app.js --name 'document-management'",
    "run-docker": "cross-env PORT=3006 NODE_ENV=production node build/app.js",
    "start": "run-s build run-prod",
    "run-dev": "cross-env PORT=3006 NODE_ENV=development nodemon --exec 'node --inspect=localhost:2006 build/app.js'",
    "bundle": "webpack --config webpack.prod.js --env=Client_Builds"
  },
  "author": "Algaeh technologies private limited.",
  "license": "ISC",
  "dependencies": {
    "algaeh-mysql": "^1.5.1",
    "base64-img": "^1.0.4",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "connect-busboy": "0.0.2",
    "cors": "^2.8.4",
    "cross-env": "^5.1.4",
    "express": "^4.16.3",
    "formidable": "^1.2.2",
    "fs-extra": "^9.0.1",
    "mime-types": "^2.1.27",
    "moment": "^2.27.0",
    "mongoose": "^5.10.0",
    "mongoose-paginate-v2": "^1.3.9",
    "rotating-file-stream": "^1.3.6",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^3.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "babel-loader": "^8.1.0",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.5",
    "obfuscator-loader": "^1.1.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
