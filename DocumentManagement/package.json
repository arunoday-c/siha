{
  "name": "documentmanagement",
  "version": "1.0.0",
  "description": "Algaeh document management",
  "main": "dist",
  "scripts": {
    "document_server": "cross-env  PORT=3006 NODE_ENV=development nodemon -w src --exec \"babel-node src/document_management_server.js \"",
    "prod_build": "babel src -s -D -d dist ",
    "dev": "npm run document_server",
    "lint": "eslint src && eslint test",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pm2": "concurrently --kill-others  cross-env NODE_ENV=production  pm2 start  --max-memory-restart 850M \"babel-node src/document_management_server.js \""
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-eval": 0,
      "no-console": 0,
      "no-unused-vars": 1,
      "no-script-url": 0
    }
  },
  "author": "Algaeh technologies private limited.",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.4",
    "express": "^4.16.3",
    "moment": "^2.22.1",
    "mongoose": "^5.3.12",
    "rotating-file-stream": "^1.3.6",
    "winston": "^2.4.2",
    "winston-daily-rotate-file": "^3.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.19.1",
    "nodemon": "^1.17.3"
  }
}
