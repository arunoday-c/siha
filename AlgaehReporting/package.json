{
  "name": "algaeh-reports",
  "version": "1.0.0",
  "description": "Algaeh Reporting tool",
  "main": "dist",
  "scripts": {
    "report_server": "run-p run-webpack run-dev",
    "build": "webpack --config webpack.prod.js",
    "run-webpack": "webpack --watch --config webpack.dev.js",
    "dev": "npm run report_server",
    "run-prod": "cross-env PORT=3018 NODE_ENV=production pm2 start  -i 2 build/app.js --name 'reports'",
    "run-docker": "cross-env PORT=3018 NODE_ENV=production node build/app.js",
    "start": "run-s build run-prod",
    "run-dev": "cross-env PORT=3018 NODE_ENV=development nodemon --exec 'node build/app.js'"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "babel-loader": "^8.0.6",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.5",
    "obfuscator-loader": "^1.1.2",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-eval": 0,
      "no-console": 0,
      "no-unused-vars": 1,
      "no-script-url": 0
    }
  },
  "dependencies": {
    "algaeh-mysql": "^1.2.5",
    "blob": "0.0.5",
    "body-parser": "^1.18.2",
    "bwip-js": "^1.7.3",
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.3",
    "concurrently": "^3.5.1",
    "core-js": "^3.2.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.4",
    "cryptr": "^4.0.0",
    "easy-pdf-merge": "^0.2.1",
    "exceljs": "^1.15.0",
    "express": "^4.16.3",
    "fs-extra": "^7.0.1",
    "handlebars": "^4.1.2",
    "moment": "^2.22.1",
    "puppeteer": "1.14.0",
    "regenerator": "^0.14.2",
    "written-number": "^0.9.1",
    "xlsx": "^0.14.2",
    "xlsx-template": "^0.5.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
