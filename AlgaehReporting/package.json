{
  "name": "algaeh-reports",
  "version": "1.0.0",
  "description": "Algaeh Reporting tool",
  "main": "dist",
  "scripts": {
    "report_server": "cross-env PORT=3018 NODE_ENV=development nodemon -w src -w algaeh_report_tool --exec \"babel-node src/report_server.js \"",
    "build": "babel src -s -D -d dist ",
    "dev": "npm run report_server",
    "start": "cross-env PORT=3018 NODE_ENV=production pm2 start  -i 2 report_server.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.19.1",
    "nodemon": "^1.17.3"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-eval": 0,
      "no-console": 0,
      "no-unused-vars": 1,
      "no-script-url": 0
    }
  },
  "dependencies": {
    "algaeh-mysql": "^1.2.5",
    "blob": "0.0.5",
    "body-parser": "^1.18.2",
    "bwip-js": "^1.7.3",
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.3",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.4",
    "cryptr": "^4.0.0",
    "easy-pdf-merge": "^0.2.1",
    "exceljs": "^1.15.0",
    "express": "^4.16.3",
    "fs-extra": "^7.0.1",
    "handlebars": "^4.1.2",
    "moment": "^2.22.1",
    "puppeteer": "1.14.0",
    "written-number": "^0.9.1",
    "xlsx": "^0.14.2",
    "xlsx-template": "^0.5.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
