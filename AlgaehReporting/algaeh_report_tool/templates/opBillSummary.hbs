{{{importStyle 'css/algaehReportStyles.css'}}}

<body>
    <div class="print-body">

        <section>
            <table class="tableForData" cell-padding="0">
                <thead>
                    <tr>
                        <th>Sl No.</th>
                        <th>Service Code</th>
                        <th>Service Name</th>
                        <th>Amount</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each details}}
                    <tr>
                        <td>{{inc @index}}</td>
                        <td>{{service_type_code}}</td>
                        <td>{{service_type}}</td>
                        <td class="numberFld">{{total_amount}}</td>
                    </tr>
                    {{/each }}
  <tr class="numberFldTotalBottom">

                        <td colspan="3" class="numberFld">Total</td>
                        <td class="numberFld">{{net_payable}}</td>
                    </tr>
                </tbody>
            </table>
        </section>
        {{!-- <footer>{{{loadPage 'doctor_signature_footer.hbs' header}}}</footer> --}}
    </div>
</body>