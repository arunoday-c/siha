        </section>
        <section>
            <table class="tableForData" cell-padding="0">
                <thead>
                    <tr>
                        <th>Sl No.</th>
                        <th>Employee Code</th>
                        <th>Employee Name</th>
                        <th>Designation</th>
                        <th>Project Name</th>

                    </tr>
                </thead>
                <tbody>
                    {{#hasElement result}}
                    {{#each result}}
                    <tr>
                        <td colspan="5" class="tableSubHdg">Projects: {{project_name}}
                            <span class="textFloatRight">Total Cost: <b>{{project_cost}}</b></span><span
                                class="textFloatRight">No Hours:
                                <b>{{no_hours}}</b></span><span class="textFloatRight">Total No Employees:
                                <b>{{no_employees}}</b></span>
                        </td>
                    </tr>
                    {{#each desig_wise_employees}}
                    <tr>
                        <td colspan="5" class="tableSubHdg0">Designation: {{designation}}
                            <span class="textFloatRight">Total No Employees: <b>{{no_employees}}</b></span >
                            {{!-- <span
                                class="textFloatRight">No
                                Projects: <b>{{no_projects}}</b></span> --}}
                        </td>
                    </tr>

                    {{#each employees}}
                    <tr>
                        <td>{{inc @index}}</td>
                        <td>{{employee_code}}</td>
                        <td style="text-align: left;">{{full_name}}</td>
                        <td>{{designation}}</td>
                        <td>{{project_desc}}</td>
                    </tr>
                    {{/each }}
                    {{/each }}
                    {{/each }}
                    {{else}}
                    <tr>
                        <td colspan="5" class="noResultTextReport">No Records Found</td>
                    </tr>{{/hasElement }}
                </tbody>

            </table>
        </section>
        {{!-- <footer>{{{loadPage 'doctor_signature_footer.hbs' header}}}</footer> --}}

    </div>
