        <section>
            <table class="tableForData" cell-padding="0">
                <thead>
                    <tr>
                        <th width="30">Sl No.</th>
                        <th width="80">Employee Code</th>
                        <th>Employee Name</th>
                        <th width="200">Designation</th>
                        <th width="200">Project Name</th>

                    </tr>
                </thead>
                <tbody>
                    {{#hasElement result}}
                    {{#each result}}
                    <tr>
                        <td colspan="4" class="tableSubHdg0">Projects: {{project_name}}</td>
                        <td colspan="1" class="tableSubHdg0 numberFld">Total:{{no_employees}}</span>
                        </td>
                    </tr>
                    {{#each desig_wise_employees}}



                    <tr>
                        <td colspan="4" class="tableSubHdg">Designation: {{designation}}</td>
                        <td colspan="1" class="tableSubHdg numberFld">Total:{{no_employees}}</span>
                        </td>
                    </tr>



                    {{#each employees}}
                    <tr>
                        <td>{{inc @index}}</td>
                        <td>{{employee_code}}</td>
                        <td style="text-align: left;">{{full_name}}</td>
                        <td>{{designation}}</td>
                        <td>{{project_desc}}</td>
                    </tr>
                    {{/each }}
                    {{/each }}
                    {{/each }}
                    {{else}}
                    <tr>
                        <td colspan="5" class="noResultTextReport">No Records Found</td>
                    </tr>{{/hasElement }}
                </tbody>

            </table>
        </section>
