{{{importStyle 'css/algaehReportStyles.css'}}}
<div class="barcodeLine">

  <small class="small-top-left">{{header.full_name}}</small>
  <img src={{barcode "code128" header.lab_id_number false }} />
  <small class="small-left"><b>{{header.patient_code}}</b></small>
  <small class="small-right">{{header.age_in_years}}Y {{header.age_in_months}}M {{header.age_in_days}}D /
    {{header.gender}}</small>
</div>