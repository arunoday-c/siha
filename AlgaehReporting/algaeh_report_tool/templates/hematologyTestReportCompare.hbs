{{{importStyle 'css/creditinvoice.css'}}}
<div class="print-body">
    <section>
        <table class="tableForLabel" cell-padding="0">
            <tr>
                <td><label>رقم الملف الطبي / Patient Reg. Number</label></td>
                <td> <span>{{header.patient_code}}</span></td>
                <td><label>اسم المريض / Patient Name</label></td>
                <td><span>{{header.patient_name}}</span></td>
            </tr>

            <tr>
                <td><label> اسم الطبيب المرجعي / Doctor Ordered by</label></td>
                <td> <span>{{header.doctor_name}} - {{header.sub_department_name}}</span></td>

                <td><label> اسم الطبيب المرجعي / Ordered Date</label></td>
                <td> <span>{{header.ordered_date}}</span></td>
            </tr>
            <tr>
                <td colspan="4" style="background:#f2f2f2;height:0px;"></td>
            </tr>
            <tr>
                <td><label>اسم اختبار / Test Name</label></td>
                <td colspan="3"> <span>{{header.service_name}}</span></td>
            </tr>
        </table>
    </section>
    <section>

        <table class="tableForDataNoBorder" cell-padding="0">
            <thead>
                <tr>
                    {{!-- <th>Sl No.</th> --}}
                    <th>Analyte</th>
                    <th>Current Result</th>
                    <th>Previous Result</th>
                    <th>Status</th>
                    <th>Normal Range</th>
                    <th>Unit</th>
                </tr>
            </thead>
            <tbody>

                {{#each details}}
                <tr>
                    <td colspan="5" class="tableSubHdg">
                        {{!-- <small>Specimen Name:</small> --}}
                        {{firstElement groupDetail 0 "investigation_name"}}</td>
                </tr>
                {{#each groupDetail}}
                <tr>
                    {{!-- <td>{{inc @index}}</td> --}}
                    <td>{{analyte_name}}</td>
                    <td data-value="{{critical_type}}">{{result}} <sup>{{critical_type}}</sup></td>
                    <td data-value="{{critical_type}}">{{result_previous}} <sup>{{critical_type}}</sup></td>
                    <td>{{result_Status}}</td>
                    <td>{{normal_low }} - {{normal_high}}</td>
                    <td>{{result_unit}}</td>
                </tr>
                {{/each}}
                {{/each}}
            </tbody>
        </table>
        <table class="tableForData">
            <tbody>
                <tr>
                    <td class="txtFld">Comments: {{header.comments}}</td>

                </tr>
                <tr>
                    <td class="txtFld">Validated By: <b>{{header.validated_by}}</b></td>

                </tr>
            </tbody>
        </table>
    </section>
    {{!-- <footer>{{{loadPage 'doctor_signature_footer.hbs' header}}}</footer> --}}
</div>