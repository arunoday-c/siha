

{{{importStyle 'css/algaehReportStyles.css'}}}
{{#ifExist totalRecords}}

    <div class="print-body" >
   <div id="total_records">{{totalRecords}}</div>
    <section>
      {{/ifExist}}
      <table class="tableForDataNoBorder" cell-padding="0">
        <thead>
          <tr>
            <th width="100px" class="txtFld">Date</th>
            <th class="txtFld">Particulars</th>
            <th width="100px" class="txtFld">Voucher Type</th>
            <th width="100px" class="txtFld">Voucher No.</th>
            <th width="80px" class="numberFld">Debit Amt</th>
            <th width="80px" class="numberFld">Credit Amt</th>
          </tr>
        </thead>
        <tbody>
          {{#each details}}
            <tr>
              <td class="txtFld" datatype="string">{{payment_date}}</td>
              <td class="txtFld" datatype="string">{{child_name}}</td>
              <td class="txtFld" datatype="string">{{voucher_type}}</td>
              <td class="txtFld" datatype="string">{{voucher_no}}</td>
              <td class="numberFld">{{debit_amount}}</td>
              <td class="numberFld">{{credit_amount}}</td>
              {{!-- <td class="numberFld ">{{numberFormating debit_amount ../currencyOnly}}</td>
                        <td class="numberFld ">{{numberFormating credit_amount ../currencyOnly}}</td> --}}
            </tr>

          {{/each}}


          <tr class="numberFld">
            <td class="txtFld" colspan="4">Closing Balance</td>
            <td class="numberFld">{{total_debit}}</td>
            <td class="numberFld">{{total_credit}}</td>
            
          </tr>
        
        </tbody>
      </table>
      {{#ifExist totalRecords}}
    </section>
  </div>
   {{/ifExist}}
