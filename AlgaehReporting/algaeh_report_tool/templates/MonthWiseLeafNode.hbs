{{{importStyle 'css/algaehReportStyles.css'}}}

<body>
    <div class="print-body">

        <section>
            <table class="tableForData" cell-padding="0">
                <thead>
                    <tr>
                        <th width="">Month </th>
                        <th width="">Debit Amt</th>
                        <th width="">Credit Amt</th>
                    </tr>
                </thead>
                <tbody>
                 
                    {{#hasElement details}}
                 
                    <tr>
                        <td colspan="3" class="tableSubHdg txtFld">{{account_name}}</td>
                    </tr>
              {{#each details}}
                    <tr>
                        <td class="">{{month_name}}</td>
                        <td class="numberFld ">{{debit_amount}}</td>
                        <td class="numberFld ">{{credit_amount}}</td>
                    </tr>
                       {{/each }}
                  
                    {{/hasElement }}


                    <tr class="numberFld ">
                        <td class="numberFld" colspan=""></td>
                        <td class="numberFld">{{total_debit}}</td>
                        <td class="numberFld">{{total_credit}}</td>
                    </tr>
                    <tr class="numberFld ">
                        <td class="numberFld" colspan="">Closing Balance</td>
                        <td class="numberFld"></td>
                        <td class="numberFld">CL amount</td>
                    </tr>
                    <tr class="numberFld numberFldTotalBottom">
                        <td class="numberFld" colspan=""></td>
                        <td class="numberFld">{{total_debit}}</td>
                        <td class="numberFld">{{total_credit}}</td>
                    </tr>
                </tbody>
            </table>


        </section>




   
    </div>

</body>