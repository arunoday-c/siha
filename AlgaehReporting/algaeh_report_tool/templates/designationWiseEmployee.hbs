{{{importStyle "css/algaehReportStyles.css"}}}

<body>
  <div class="print-body">

    {{!-- <section>
            <table class="tableForLabel" cell-padding="0">
                <tr>
                    <td><label class="textEnLft">Branch</label>:<span class="valueText">{{hospital_name}}</span></td>
                    <td><label class="textEnLft">No Employees</label>:<span class="valueText">{{no_employees}}</span>
                    </td>
                </tr>
            </table>
        </section> --}}

    <section>
      <table class="tableForData" cell-padding="0">
        <thead>
          <tr>
            <th>Sl No.</th>
            <th>Primary ID</th>
            <th>Employee Code</th>
            <th>Employee Name</th>
            <th>Gender</th>
            <th>Emp. Group</th>
            <th>Dept. </th>
            <th>Sub Dept.</th>
            <th>Date of Joining</th>
            <th>Nationality</th>
            <th>Religion</th>
            <th>Emp. Type</th>
            <th>Emp. Status</th>
          </tr>
        </thead>
        <tbody>
          {{#hasElement result}}

            {{#each result}}
              <tr>
                <td colspan="13" class="tableSubHdg">Designation:
                  {{designation}}
                  <span class="textFloatRight">No Employees:
                    {{no_employee}}</span>
                </td>
              </tr>
              {{#each employees}}
                <tr>
                  <td datatype="string">{{inc @index}}</td>
                  <td datatype="string">{{identity_no}}</td>
                  <td datatype="string">{{employee_code}}</td>
                  <td style="text-align: left;">{{full_name}}</td>
                  <td>{{sex}}</td>
                  <td>{{group_description}}</td>
                  <td>{{department_name}}</td>
                  <td>{{sub_department_name}}</td>
                  <td>{{date_of_joining}}</td>
                  <td>{{nationality}}</td>
                  <td>{{religion_name}}</td>
                  <td>{{employee_type}}</td>
                  <td>{{asemployee_status}}</td>
                </tr>
              {{/each}}{{/each}}
          {{else}}
            <tr>
              <td colspan="13" class="noResultTextReport">No Records Found</td>
            </tr>{{/hasElement}}
        </tbody>

      </table>
    </section>
    {{!-- <footer>{{{loadPage 'doctor_signature_footer.hbs' header}}}</footer> --}}

  </div>
</body>