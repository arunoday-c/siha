        <section>
            <table class="tableForData" cell-padding="0">
                <thead>
                    <tr>
                    <tr>
                        <th>Sl No.</th>
                        <th>Item Code</th>
                        <th>Item Name</th>
                        <th>UOM</th>
                        <th>Requested Qty</th>
                        <th>Unit Price</th>
                        <th>Total Cost</th>
                    </tr>
                    </tr>
                </thead>
                <tbody>
                    {{#hasElement result}}

                    {{#each result}}
                    <tr>
                        <td colspan="7" class="tableSubHdg">PO No.: {{inc @index}}.
                            {{purchase_number}} / Supplier :
                            {{vendor_name}}
                            <span class="textFloatRight"><b>{{net_total}}</b></b></span>
                        </td>

                    </tr>
                    {{#each poitems}}
                    <tr>
                        <td>{{inc @index}}</td>
                        <td>{{item_code}}</th>
                        <td class="txtFld">{{item_description}}</th>
                        <td>{{uom_description}}</td>
                        <td class="numberFld">{{authorize_quantity}}</td>
                        <td class="numberFld">{{unit_cost}}</td>
                        <td class="numberFld">{{net_extended_cost}}</td>
                    </tr>
                    {{/each }}{{/each }}
                    {{else}}
                    <tr>
                        <td colspan="7" class="noResultTextReport">No Records Found</td>
                    </tr>{{/hasElement }}

                    <tr class="numberFldTotalBottom">
                        <td colspan="6"></td>
                        <td class="numberFld">{{net_total}}</td>
                    </tr>
                </tbody>

            </table>
        </section>
        {{!-- <footer>{{{loadPage 'doctor_signature_footer.hbs' header}}}</footer> --}}

    </div>
</body>