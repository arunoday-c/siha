        <section>
            <table class="tableForData" cell-padding="0">
                <thead>
                    <tr>
                    <tr>
                        <th>Sl No.</th>
                          <th>category</th>
                        <th>Item Code</th>
                        <th>Item Name</th>
                        <th>UOM</th>
                        <th>Requested Qty</th>
                        <th>Unit Price</th>
                        <th>Total Cost</th>
                    </tr>
                    </tr>
                </thead>
                <tbody>
                    {{#hasElement result}}

                    {{#each result}}
                    <tr>
                        <td colspan="4" class="tableSubHdg">{{purchase_number}} / {{po_date}}</td>
                         <td colspan="3" class="tableSubHdg numberFld">
                          {{vendor_name}}
                           {{!-- | <span><b>{{net_total}}</b></span> --}}
                        </td>
                       

                    </tr>
                    {{#each poitems}}
                    <tr>
                        <td>{{inc @index}}</td>
                        <td>{{category_desc}}</td>
                        <td>{{item_code}}</th>
                        <td class="txtFld">{{item_description}}</th>
                        <td>{{uom_description}}</td>
                        <td class="numberFld">{{authorize_quantity}}</td>
                        <td class="numberFld">{{numberFormating unit_cost ../../currencyOnly}}</td>
                        <td class="numberFld">{{numberFormating net_extended_cost ../../currencyOnly}}</td>
                    </tr>
                    {{/each }} <tr class="tableSubHdg2">
                        <td colspan="6"></td>
                        <td class="numberFld"><b>{{net_total}}</b></td>
                    </tr>{{/each }}
                   
                    {{else}}
                    <tr>
                        <td colspan="7" class="noResultTextReport">No Records Found</td>
                    </tr>{{/hasElement }}

                </tbody>

            </table>
        </section>
