{{{importStyle 'css/algaehReportStyles.css'}}}

<body>
    <div class="print-body">

        <section>

            <table class="tableForLabel" cell-padding="0">
                <tr>
                    <td class="textEngLft" style="width:20%">Invoice No.</td>
                    <td colspan="2" class="textEngCtr" style="width:60%"><b>{{invoice_number}}</b></td>
                    <td class="textArbRht" style="width:20%">رقم الفاتورة</td>
                </tr>
                <tr>
                    <td class="textEngLft" style="width:20%">Date</td>
                    <td colspan="2" class="textEngCtr" style="width:50%"><b>{{invoice_date}}</b></td>
                    <td class="textArbRht" style="width:20%">التاريخ</td>
                </tr>
                <tr>
                    <td class="textEngLft" style="width:20%">Customer</td>
                    <td class="textEngLft" style="width:30%"><b>{{customer_name}}</b></td>
                    <td class="textArbRht" style="width:30%"><b>{{arabic_customer_name}}</b></td>
                    <td class="textArbRht" style="width:20%">العميل</td>
                </tr>
                <tr>
                    <td class="textEngLft" style="width:20%">Address</td>
                    <td colspan="2" class="textEngCtr" style="width:60%"><b>{{address}}</b></td>
                    <td class="textArbRht" style="width:20%">العنوان</td>
                </tr>
                <tr>
                    <td class="textEngLft" style="width:20%">PO No./Contarct No.</td>
                    <td colspan="2" class="textEngCtr" style="width:60%"><b>{{customer_po_no}}</b></td>
                    <td class="textArbRht" style="width:20%">:رقم الطلب</td>
                </tr>
                <tr>
                    <td class="textEngLft" style="width:20%">VAT No.</td>
                    <td colspan="2" class="textEngCtr" style="width:60%"><b>{{vat_number}}</b></td>
                    <td class="textArbRht" style="width:20%">الرقم الضريبي</td>
                </tr>
                <tr>
                    <td class="textEngLft" style="width:20%">Site</td>
                    <td colspan="2" class="textEngCtr" style="width:60%"><b>{{hospital_name}}</b></td>
                    <td class="textArbRht" style="width:20%">الموقع</td>
                </tr>
                  <tr>
                    <td colspan="4" class="textEngCtr" style="width:100%"><b>{{invoice_status}}</b></td>
                </tr>
            </table>




        </section>
        <section>
            <table class="tableForData" cell-padding="0">
                <thead>
                    <tr>
                        <th>Sl No.</th>
                        <th>Service Name/اسم الخدمة</th>
                        <th>Quantity/ الكمية</th>
                        <th>Rate/ سعر الوحدة</th>
                        <th>Vat %/ الضريبة</th>
                        <th>Amount/ الإجمالي</th>
                    </tr>
                </thead>
                <tbody>

                    {{#each detailList}}
                    <tr>
                        {{!-- <td>{{../currency.decimal_places}}</td> --}}
                        <td datatype="string">{{inc @index}}</td>
                        <td  class="txtFld">{{service_name}} - {{comments}} <br /> {{arabic_service_name}} - {{arabic_comments}}</td>

                         <td datatype="string">{{quantity}}</td>
                        <td class="numberFld">
                            {{numberFormating unit_cost ../currency}}
                        </td>
                        <td>{{tax_percentage}}%</td>
                        <td class="numberFld">{{numberFormating total_amount ../currency}}</td>
                        {{!-- <td>{{arabic_service_name}}</td> --}}
                    </tr>
                    {{/each }}</tbody>
            </table>
        </section>

