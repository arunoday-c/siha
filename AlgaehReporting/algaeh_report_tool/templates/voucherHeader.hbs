<style type="text/css">
  .reportHeaderCntr { width: 100%; border-collapse: collapse; margin: 0 18px
  18px; font-family: Arial, sans-serif; font-weight: normal; font-size: 11px;
  overflow: hidden; } .col-left { float: left; width: 25%; text-align: left; }
  .col-center { width: 50%; text-align: center; float: none; margin: 0 auto; }
  .col-right { float: right; width: 25%; text-align: right; } .hospitalLogo img
  { margin: 0; padding: 0; border: none; height: 35px; width: auto; text-align:
  center; } .reportHeaderCntr h5 { margin: 3px 0; padding: 0; font-size: 11px; }
  .reportHeaderCntr p { margin: 2px 0; padding: 0; font-size: 7px; line-height:
  10px; } .reportHeaderCntr p b { font-size: 8px; } .showFilterValue { width:
  100%; } .showFilterValue span { font-weight: bold; display: inline-block;
  padding: 3px 5px; border-left: 1px solid #ccc; text-align: left; }
  .showFilterValue span:first-child { border-left: none; } .showFilterValue
  small { display: block; color: #ccc; } .headerFooterStyle { width: 100%;
  height: 10px; float: left; text-align: center; border-top: 0.5rem dotted #ccc;
  font-size: 8px; margin-top: 5px; }
</style>

<body>

  <div class="reportHeaderCntr">
    <div class="col-left">
      <p><b>{{organization_name}}</b></p>
      <p>{{address1}}</p>
      <p>Tel: <span>{{phone1}}</span></p>
      <p>Vat No: <span>{{tax_number}}</span></p>
    </div>
    <div class="col-right">
      <p><b>{{legal_name}}</b></p>
      <p>{{address2}}</p>
      <p><span style="display: inline-block; text-align:left;">{{phone2}}
          :</span>رقم الهاتف</p>
      <p>رقم الضريبة المضافة: <span>{{tax_number}}</span></p>
    </div>

    <div class="col-center">
      <p class="hospitalLogo"><img src="{{logoUrl "ORG" identity}}" /></p>
      {{!-- <h5>{{{report_name_for_header}}}</h5> --}}
    </div>
    <div class="headerFooterStyle">
      <p class="showFilterValue">

        {{!-- {{#each specialHeader}}
                <span><small>{{name}}:</small>{{value}} </span>
                {{/each }} --}}
      </p>
    </div>
  </div>

</body>