{{{importStyle "css/algaehReportStyles.css"}}}

<body>
  <div class="print-body">

    <section>
      {{!-- <table class="tableForLabel" cell-padding="0">
                <tr>
                    <td><label class="textEnLft">Total Employees</label>:<span class="valueText">{{no_employees}}</span>
                    </td>
                </tr>
            </table> --}}

    </section>
    <section>
      <table class="tableForData" cell-padding="0">
        <thead>
          <tr>
            <th>Sl No.</th>
            <th>Service Code</th>
            <th>Service Name</th>
            <th>Unit Cost</th>
            <th>Qty</th>
            <th>Patient Tax</th>
            <th>Patient Payable</th>
            <th>Company Tax</th>
            <th>Company Payable</th>
            <th>Net Amount</th>
          </tr>

        </thead>
        <tbody>
          {{!-- <tr>
                        <td colspan="10"></td>
                        <td colspan="1"><span class="textFloatRight">Total No.
                                Employees: <b>{{no_employees}}</b></span></td>

                    </tr> --}}
          {{#hasElement detail}}

           
  <tr class="numberFld numberFldTotalBottom">
                  <td class="numberFld" colspan="9">
                  
                  </td>
                   <td class="numberFld">
                   {{numberFormating total_net_amount_total ./currencyOnly}}
                  </td>
                 </tr>
           {{#each detail}}
             
           
                <tr>
                  <td colspan="9" class="tableSubHdg2">

                    {{bill_number}}/{{bill_date}}
                  </td>
                  <td class="tableSubHdg2 numberFld"><b>{{numberFormating net_amount_total ../decimalOnly}} </b></td>
                </tr>
                {{#each detailList}}
                  <tr>
                    <td datatype="string">{{inc @index}}</td>
                    <td class="">{{service_code}}</td>
                    <td class="txtFld" datatype="string">{{service_name}}</td>
                    <td class="numberFld">{{unit_cost}}</td>
                    <td class="numberFld">{{quantity}}</td>
                    <td class="numberFld">{{patient_tax}}</td>
                    <td class="numberFld">{{patient_resp}}</td>
                    <td class="numberFld">{{company_tax}}</td>
                    <td class="numberFld">{{comapany_resp}}</td>
                    <td class="numberFld"><b>{{net_amout}}</b></td>
                  </tr>
                {{/each}}
                  {{!-- <tr class="footerTotalDown">
                        <td colspan="4" class="numberFld"></td>
                        <td class="numberFld">{{sum_amount}}</td>
                    </tr> --}}
                {{/each}}
           <tr class="numberFld numberFldTotalBottom">
                  <td class="numberFld" colspan="9">
                  
                  </td>
                   <td class="numberFld">
                   {{numberFormating total_net_amount_total ./currencyOnly}}
                  </td>
                 </tr>
          {{else}}
            <tr>
              <td colspan="10" class="noResultTextReport">No Records Found</td>
            </tr>{{/hasElement}}
        </tbody>

      </table>
    </section>
    {{!-- <footer>{{{loadPage 'doctor_signature_footer.hbs' header}}}</footer> --}}

  </div>
</body>