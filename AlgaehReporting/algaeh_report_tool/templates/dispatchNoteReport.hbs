{{{importStyle 'css/algaehReportStyles.css'}}}

<body>
    <div class="print-body">

        <section>

            <table class="tableForLabel" cell-padding="0">

                <tr>
                    <td><label class="textEnLft">Dispatch No.</label>:<span
                            class="valueText">{{dispatch_note_number}}</span></td>
                    <td><label class="textEnLft">Dispatch From</label>:<span
                            class="valueText">{{location_description}}</span></td>

                </tr>
                <tr>
                    <td><label class="textEnLft">Dispatch Date</label>:<span
                            class="valueText">{{dispatch_note_date}}</span>
                    </td>
                    <td><label class="textEnLft">Customer Name</label>:<span class="valueText">{{customer_name}}</span>
                    </td>
                </tr>

                <tr>
                    <td><label class="textEnLft">Order No</label>:<span class="valueText">{{sales_order_number}}</span>
                    </td>
                    <td><label class="textEnLft">Order Date</label>:<span class="valueText">{{sales_order_date}}</span>
                    </td>
                </tr>

            </table>


        </section>
        <section>
            <table class="tableForData" cell-padding="0">
                <thead>
                    <tr>
                        <th>Sl No.</th>
                        <th>Batch No.</th>
                        <th>Quantity</th>
                        <th>Unit Cost</th>
                        <th>Sales Price</th>
                        <th>Discount %</th>
                        <th>Discount Amt.</th>
                        <th>Net Ext. Cost</th>
                        <th>Tax Amt.</th>
                        <th>Total Amt.</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each details}}
                    <tr>
                        <td colspan="5" class="tableSubHdg"><small>Item Name:</small><br>{{item_description}} </td>
                        <td class="tableSubHdg"><small>Item Code:</small><br> {{item_code}}</td>
                        <td class="tableSubHdg"><small>Purchase UOM:</small><br>{{uom_description}}</td>
                        <td class="tableSubHdg"><small>Req. Qty:</small><br>{{ordered_quantity}}</td>
                        <td class="tableSubHdg"><small>Deliverd Qty:</small><br>{{dispatched_quantity}}</td>
                        <td class="tableSubHdg"><small>Out Standing Qty:</small><br>{{quantity_outstanding}}</td>
                    </tr>
                    {{#each batches}}
                    <tr>
                        <td>{{inc @index}}</td>
                        <td>{{batchno}}</td>
                        <td>{{dispatch_quantity}}</td>
                        <td>{{unit_cost}}</td>
                        <td>{{sales_price}}</td>
                        <td>{{discount_percentage}}</td>
                        <td>{{discount_amount}}</td>
                        <td>{{net_extended_cost}}</td>
                        <td>{{tax_amount}}</td>
                        <td>{{total_amount}}</td>
                    </tr>
                    {{/each }}

                    {{/each }}<tr class="footerTotalDown">
                        <td colspan="9">Total Net Amount</td>
                        <td>{{net_payable}}</td>
                    </tr>
                </tbody>
            </table>
        </section>
        {{!-- <footer>{{{loadPage 'doctor_signature_footer.hbs' header}}}</footer> --}}
    </div>
</body>