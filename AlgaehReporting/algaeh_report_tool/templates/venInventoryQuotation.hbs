{{{importStyle 'css/algaehReportStyles.css'}}}

<body>
    <div class="print-body">

        <section>

            <table class="tableForLabel" cell-padding="0">

                <tr>
                    <td><label class="textEnLft">Vendor Quotation No</label>:<span
                            class="valueText">{{vendor_quotation_number}}</span>
                    </td>
                    <td><label class="textEnLft">Transation Date</label>:<span
                            class="valueText">{{vendor_quotation_date }}</span>
                    </td>
                </tr>
                <tr>
                    <td><label class="textEnLft">Vendor</label>:<span class="valueText">{{vendor_name }}</span>
                    </td>

                    <td><label class="textEnLft">Expected Date</label>:<span class="valueText">{{expected_date }}</span>
                    </td>
                </tr>
                <tr>
                    <td><label class="textEnLft">Req Quotation No</label>:<span
                            class="valueText">{{quotation_number }}</span>
                    </td>


                </tr>
            </table>


        </section>
        <section>
            <table class="tableForData" cell-padding="0">
                <thead>
                    <tr>
                        <th>Sl No.</th>
                        <th>Item Name</th>
                        <th>Item Code</th>
                        <th>UOM</th>
                        <th>Qty</th>
                        <th>Unit Price</th>
                        <th>Extended Price</th>
                        <th>Discount %</th>
                        <th>Discount Amount</th>
                        <th>Net Extended Amount</th>
                        <th>Tax %</th>
                        <th>Tax Amount</th>
                        <th>Total Amount</th>
                    </tr>
                </thead>
                <tbody>

                    {{#each detailList}}
                    <tr>
                        <td>{{inc @index}}</td>
                        <td>{{item_description}}</td>
                        <td>{{item_code}}</td>
                        <td>{{uom_description}}</td>
                        <td>{{quantity}}</td>
                        <td>{{unit_price}}</td>
                        <td>{{extended_price}}</td>
                        <td>{{discount_percentage}}</td>
                        <td>{{discount_amount}}</td>
                        <td>{{net_extended_cost}}</td>
                        <td>{{tax_percentage}}</td>
                        <td>{{tax_amount}}</td>
                        <td>{{total_amount}}</td>

                    </tr>
                    {{/each }}
                </tbody>
            </table>
        </section>
        {{!-- <footer>{{{loadPage 'doctor_signature_footer.hbs' header}}}</footer> --}}
    </div>
</body>