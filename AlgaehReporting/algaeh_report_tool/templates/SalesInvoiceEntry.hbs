        <table class="tableTotal">
            <tr>
                <td>Total Amt.</td>
                <td>{{sub_total}}</td>
            </tr>
            <tr>
                <td>Total Discount</td>
                <td>{{discount_amount}}</td>
            </tr>
            <tr>
                <td>Net Total</td>
                <td>{{net_total}}</td>
            </tr>
            <tr>
                <td>Total Tax</td>
                <td>{{total_tax}}</td>
            </tr>
            <tr class="bold">
                <td>Total Net Recivable</td>
                <td>{{net_payable}}</td>
            </tr>
        </table>
