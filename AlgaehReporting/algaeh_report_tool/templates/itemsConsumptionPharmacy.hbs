{{{importStyle 'css/algaehReportStyles.css'}}}

<body>
    <div class="print-body">

        <section>
            <table class="tableForData" cell-padding="0">
                <thead>
                     <tr>
    <th>Sl No.</th>
    <th>Cons. Date</th>
    <th>Cons. Code</th>
    <th>category</th>
    <th>Item Code</th>
    <th>Item Name</th>
    <th>Department</th>
    <th>Qty</th>
    <th>Unit Price</th>
    <th>Net Amount</th>
  </tr>
                </thead>
                <tbody>
            {{#each detail}}
                    <tr class="tableSubHdg">
                        <td colspan="9"  class="txtFld">{{doctor}}</td>
                    </tr>
                    {{#each detailList}}
                    <tr>
                        <td datatype="string">{{inc @index}}</td>
                        <td>{{consumption_date}}</td>
                        <td>{{consumption_number}}</td>
                        <td>{{category_desc}}</td>
                        <td>{{item_code}}</td>
                        <td>{{item_description}}</td>
                        <td>{{sub_department_name}}</td>
                         <td datatype="string">{{quantity}}</td>
                        <td class="numberFld">{{numberFormating unit_cost ../../currencyOnly}}</td>
                        <td class="numberFld">{{numberFormating extended_cost ../../currencyOnly}}</td>
                    </tr>
                    {{/each }}
                    <tr class="tableSubHdg2">
                        <td colspan="8" class="numberFld">Total Net Amount</td>
                        <td class="numberFld">{{numberFormating total_cost ../currencyOnly}}</td>
                    </tr>
                    {{/each }}
              
                </tbody>
            </table>
            <table class="tableTotal">
                <tr class="bold"><td>Grand Total</td><td>{{numberFormating grandTotal currencyOnly}}</td><td>المبلغ الإجمالي الكلي</td></tr>
               </table>
        </section>
        {{!-- <footer>{{{loadPage 'doctor_signature_footer.hbs' header}}}</footer> --}}
    </div>
</body>