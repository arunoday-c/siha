{{{importStyle "css/algaehReportStyles.css"}}}

<style type="text/css">
  @media print { .print-body { font: 13pt "Calibri", sans-serif; line-height:
  1.3; background: #fff !important; color: #000; } }
</style>

<div class="print-body">
  {{!-- {{#each header}} --}}
  <section class="tableForPharmacyDiv">

    <table class="tableForPharmacy" cell-padding="0">
      <tbody>
        <tr>
          <td>Dispense date</td>
          <td>{{dispense_date}}</td>
          <td>{{dispense_date}}</td>
          <td>تاريخ صرف الدواء</td>
        </tr>
        {{!-- <tr>
          <td>Pharmacy Name</td>
          <td>{{hospital_name}}</td>
          <td>{{arabic_hospital_name}}</td>
          <td>اسم الصيدلية</td>
        </tr> --}}
        <tr>
          <td>Patient Name</td>
          <td>{{patient_full_name}}</td>
          <td>{{patient_arabic_name}}</td>
          <td>اسم المريض</td>
        </tr>
        <tr>
          <td>Medcine Name</td>
          <td>{{item_description}}</td>
          <td>{{item_description}}</td>
          <td>اسم الدواء</td>
        </tr>
        <tr>
          <td>Dosage</td>
          <td>{{instructions}}</td>
          <td>{{instructions}}</td>
          <td>الجرعة</td>
        </tr>

        <tr>
          <td>Expiry Date</td>
          <td>{{expiry_date}}</td>
          <td>{{expiry_date}}</td>
          <td>تاريخ الانتهاء</td>
        </tr>
        <tr>
          <td>Pharmacist Name</td>
          <td>{{p_name}}</td>
          <td>{{p_arabic_name}}</td>
          <td>اسم الصيدلي</td>
        </tr>

      </tbody>
    </table>
  </section>
