{{{importStyle 'css/algaehReportStyles.css'}}}

<div class="print-body">
{{#each records}}
    <table class="tableForDataNoBorder" cell-padding="0">
        <tbody>
        <tr class="tableSubHdg2" >
            <td colspan="4" class="txtFld">
                <b>{{service_name}}</b> - ({{investigation_name}})
            </td>
           
        </tr>
        {{#each details}}
         {{#if order 1 }}
            <tr style="page-break-after: auto;">
                <td colspan="4" class="tableSubHdg2 txtFld" style="background-color:rgb(255, 255, 255);"><i>{{analyte_report_group_desc}}</i></td>
            </tr>
         {{/if}}
        {{#each groupDetails}}
       <tr>
            {{!-- <td>{{inc @index}}</td> --}}
            <td width="40%" class="txtFld">{{analyte_name}}</td>
            <td width="20%" class="numberFld" data-value="{{critical_type}}">{{result}} <sup>{{critical_type}}</sup></td>
            <td width="20%" class="numberFld">{{#if reference_range_required "Y"}} {{result_unit}} {{else}} -- {{/if}}</td>
            <td width="20%" class="numberFld">
                {{#if reference_range_required "Y"}} 
                    {{#if analyte_type "T"}} 
                        <ul class="rangeUl"> 
                        {{#each (commentBreakUp text_value commentBreakUp)}}
                        <li>{{this}}</li>
                        {{/each}}
                        </ul>
                    {{else}}
                    {{normal_low }} - {{normal_high}}{{/if}}
                {{else}} -- {{/if}}
            </td>
        </tr>
        {{/each}}
        {{/each}}
        </tbody>
    </table>
{{/each}}
</div>