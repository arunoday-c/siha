{{{importStyle "css/algaehReportStyles.css"}}}

<body>
  <div class="print-body">

    <section>
      <table class="tableForData" cell-padding="0">
        <thead>
          <tr>
            <th>Sl No.</th>
            <th>Patient Code</th>
            <th>Patient Name</th>
            <th>Age</th>
            <th>DOB</th>
            <th>Gender</th>
            <th>DOR</th>
            <th>Nationality</th>

          </tr>
        </thead>
        <tbody>
          {{#hasElement result}}
            <tr class="numberFldTotalBottom">
              <td class="txtFld" colspan="8">Total Patient: {{no_patients}}</td>
              {{!-- <td class="numberFld" colspan="3">Age: {{return_age}}</td> --}}
            </tr>
            {{#each result}}
              <tr>
                <td datatype="string">{{inc @index}}</td>
                <td>{{patient_code}}</td>
                <td style="text-align: left;">{{full_name}}</td>
                <td>{{new_age}}</td>
                <td datatype="string">{{date_of_birth}}</td>
                <td>{{gender}}</td>
                <td datatype="string">{{created_date}}</td>
                <td>{{nationality}}</td>
              </tr>
            {{/each}}
          {{else}}
            <tr>
              <td colspan="8" class="noResultTextReport">No Records Found</td>
            </tr>{{/hasElement}}
        </tbody>

      </table>
    </section>
    {{!-- <footer>{{{loadPage 'doctor_signature_footer.hbs' header}}}</footer> --}}

  </div>
</body>