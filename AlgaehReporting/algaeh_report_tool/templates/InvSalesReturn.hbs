{{{importStyle "css/algaehReportStyles.css"}}}

<body>
  <div class="print-body">

    <section>

      <table class="tableForLabel" cell-padding="0">

        <tr>
          <td><label class="textEnLft">Return No</label>:<span
              class="valueText"
            >{{sales_return_number}}</span>
          </td>
          <td><label class="textEnLft">Return To</label>:<span
              class="valueText"
            >{{location_description}}</span></td>

        </tr>
        <tr>
          <td><label class="textEnLft">Return Date</label>:<span
              class="valueText"
            >{{return_date}}</span>
          </td>
          <td><label class="textEnLft">Customer Name</label>:<span
              class="valueText"
            >{{customer_name}}</span>
          </td>
        </tr>

      </table>

    </section>
    <section>
      <table class="tableForData" cell-padding="0">
        <thead>
          <tr>
            <th>Sl No.</th>
            <th>Item Name</th>
            <th>Unit</th>
            <th>Batch No.</th>
            <th>Dispatched Qty</th>
            <th>Return Qty</th>
            <th>Unit Price</th>
            <th>Ext. Price</th>
            <th>Disc. Amt</th>
            <th>Net Ext. Amt.</th>
            <th>Tax Amt.</th>
            <th>Total Amt. </th>

          </tr>
        </thead>
        <tbody>

          {{#each detailList}}
            <tr>
              <td datatype="string">{{inc @index}}</td>
              <td>{{item_description}}</td>
              <td>{{uom_description}}</td>
              <td>{{batchno}}</td>
              <td>{{dispatch_quantity}}</td>
              <td>{{return_qty}}</td>
              <td>{{unit_cost}}</td>
              <td>{{extended_cost}}</td>
              <td>{{discount_amount}}</td>
              <td>{{net_extended_cost}}</td>
              <td>{{tax_amount}}</td>
              <td>{{total_amount}}</td>

            </tr>
          {{/each}}
          <tr class="footerTotalDown">
            <td colspan="11">Total Net Amount</td>
            <td>{{return_total}}</td>
          </tr>
        </tbody>
      </table>
    </section>
    {{!-- <footer>{{{loadPage 'doctor_signature_footer.hbs' header}}}</footer> --}}
  </div>
</body>