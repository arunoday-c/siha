{{{importStyle 'css/algaehReportStyles.css'}}}

<body>
    <div class="print-body">

        {{!-- <section>
            <table class="tableForLabel" cell-padding="0">
                <tr>
                    <td><label class="textEnLft">Branch</label>:<span class="valueText">{{hospital_name}}</span></td>
                    <td><label class="textEnLft">No Employees</label>:<span class="valueText">{{no_employees}}</span>
                    </td>
                </tr>
            </table>


        </section> --}}
        <section>
            <table class="tableForData" cell-padding="0">
                <thead>
                    <tr>
                        <th>Sl No.</th>
                        <th>Service Code</th>
                        <th>Service Name</th>
                        <th>Quantity</th>
                        <th>Unit Price</th>
                        <th>Total Cost</th>
                        {{!-- <th>From Location</th>
                        <th>To Location</th> --}}
                    </tr>
                </thead>
                <tbody>
                    {{#hasElement result}}

                    {{#each result}}
                    <tr>
                        <td colspan="3" class="tableSubHdg txtFld">{{invoice_number}} - {{invoice_date}}</td>
                        <td colspan="3" class="tableSubHdg numberFld">{{customer_name}}</td>
                      
                    </tr>
                    {{#each transfer_items}}
                    <tr>
                        <td datatype="string">{{inc @index}}</td>
                        <td>{{service_code}}</th>
                        <td class="txtFld" datatype="string">{{service_name}}</th>
                        <td class="numberFld">{{quantity}}</td>
                        <td class="numberFld">{{numberFormating unit_cost ../../currencyOnly}}</td>
                        <td class="numberFld">{{numberFormating net_extended_cost ../../currencyOnly}}</td>
                        {{!-- <td class="">{{from_location}}</td>
                        <td class="">{{to_location}}</td> --}}
                    </tr>
                    {{/each }}
                    <tr class="tableSubHdg2">
                        <td colspan="5"></td>
                        <td class="numberFld"><b>{{sum_total_net_extended_cost}}</b></td>
                    </tr>
                    {{/each }}
                    {{else}}
                    <tr>
                        <td colspan="6" class="noResultTextReport">No Records Found</td>
                    </tr>{{/hasElement }}
                </tbody>

            </table>
        </section>
        {{!-- <footer>{{{loadPage 'doctor_signature_footer.hbs' header}}}</footer> --}}

    </div>
</body>