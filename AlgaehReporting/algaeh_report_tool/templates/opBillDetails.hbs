{{{importStyle "css/algaehReportStyles.css"}}}

<body>
  <div class="print-body">

    <section>
      <table class="tableForData" cell-padding="0">
        <thead>
          <tr>
            <th>Sl No.</th>
            <th>Service Code</th>
            <th>Service Name</th>
            <th>Qty</th>
            <th>Amount</th>
          </tr>
        </thead>
        <tbody>
          {{#each detail}}
            <tr>
              <td colspan="5" class="tableSubHdg">{{service_type}}</td>
            </tr>
            {{#each detailList}}
              <tr>
                <td datatype="string">{{inc @index}}</td>
                <td>{{service_code}}</td>
                <td class="txtFld" datatype="string">{{service_name}}</td>
                <td>{{service_count}}</td>
                <td class="numberFld">{{total_amount}}</td>
              </tr>
            {{/each}}
            <tr class="footerTotalDown">
              <td colspan="4" class="numberFld"></td>
              <td class="numberFld">{{sum_amount}}</td>
            </tr>
          {{/each}}
          <tr class="numberFldTotalBottom">
            <td colspan="4"></td>
            <td class="numberFld">{{net_total}}</td>

          </tr>
        </tbody>
      </table>
    </section>
    {{!-- <footer>{{{loadPage 'doctor_signature_footer.hbs' header}}}</footer> --}}
  </div>
</body>