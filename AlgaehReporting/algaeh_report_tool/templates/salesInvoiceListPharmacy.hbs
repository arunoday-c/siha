{{{importStyle 'css/algaehReportStyles.css'}}}



<body>
    <div class="print-body">

        <section>
            <table class="tableForData" cell-padding="0">
                <thead>
                    <tr>
                        <th>Sl No.</th>
                        <th>category</th>
                        <th>Item Code</th>
                        <th>Item Name</th>
                        <th>Batch No.</th>
                        <th>Qty Sold</th>
                        <th>Expiry Date</th>
                        <th>Patient Responsibility</th>
                        <th>Patient Tax</th>
                        <th>Company Responsibility</th>
                        <th>Company Tax</th>
                        <th>Net Payable</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each detail}}
                    <tr>
                        <td colspan="11" class="tableSubHdg">{{cashier_name}} / {{pos_number}} / {{pos_date}}</td>
                    </tr>
                    {{#each detailList}}
                    <tr>
                        <td>{{inc @index}}</td>
                        <td>{{category_desc}}</td>
                        <td>{{item_code}}</td>
                        <td>{{item_description}}</td>
                        <td>{{batchno}}</td>
                        <td>{{quantity}}</td>
                        <td>{{expiry_date}}</td>

                        <td class="numberFld">{{numberFormating PD_patient_responsibility ../../decimalOnly}}</td>

                        <td class="numberFld">{{numberFormating PD_patient_tax ../../decimalOnly}}</td>

                        <td class="numberFld">{{numberFormating PD_company_responsibility ../../decimalOnly}}</td>

                        <td class="numberFld">{{numberFormating PD_company_tax ../../decimalOnly}}</td>

                        <td class="numberFld">{{numberFormating PD_net_payable ../../decimalOnly}}</td>
                       
                    </tr>
                    {{/each }}
                    <tr class="footerTotalDown">
                        <td colspan="6" class="numberFld"></td>
                       
                        <td class="numberFld">{{numberFormating net_PD_patient_responsibility ../decimalOnly}}</td>
                        <td class="numberFld">{{numberFormating net_PD_patient_tax ../decimalOnly}}</td>
                        <td class="numberFld">{{numberFormating net_PD_company_responsibility ../decimalOnly}}</td>
                        <td class="numberFld">{{numberFormating net_PD_company_tax ../decimalOnly}}</td>
                        <td class="numberFld">{{numberFormating net_PD_net_payable ../decimalOnly}}</td>
                    </tr>
                    {{/each }}
                    <tr class="numberFldTotalBottom">


  <td colspan="6" class="numberFld"></td>
   <td ></td>
                        {{!-- <td class="numberFld">{{net_amount}}</td> --}}
                        <td class="numberFld">{{numberFormating final_net_PD_patient_responsibility currencyOnly}}</td>
                        <td class="numberFld">{{numberFormating final_net_PD_patient_tax currencyOnly}}</td>
                        <td class="numberFld">{{numberFormating final_net_PD_company_responsibility currencyOnly}}</td>
                        <td class="numberFld">{{numberFormating final_net_PD_company_tax currencyOnly}}</td>
                        <td class="numberFld">{{numberFormating final_net_PD_net_payable currencyOnly}}</td>



                   


                    </tr>
                </tbody>
            </table>
        </section>
    </div>
</body>
