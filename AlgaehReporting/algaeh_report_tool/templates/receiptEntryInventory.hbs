{{{importStyle 'css/creditinvoice.css'}}}

<body>
    <div class="print-body">

        <section>

            <table class="tableForLabel" cell-padding="0">
                <tr>
                    <td><label class="textEnLft">Receipt No</label>:<span class="valueText">{{receipt_number}}</span>
                    </td>
                    <td><label class="textEnLft">Receipt Date</label>:<span class="valueText">{{receipt_date}}</span>
                    </td>

                </tr>
                <tr>
                    <td><label class="textEnLft">Invoice No</label>:<span class="valueText">{{invoice_no}}</span>
                    </td>
                    <td><label class="textEnLft">Invoice Date</label>:<span class="valueText">{{invoice_date}}</span>
                    </td>
                </tr>
                <tr>
                    <td><label class="textEnLft">Location</label>:<span
                            class="valueText">{{location_description}}</span></td>
                    <td><label class="textEnLft">Vendor</label>:<span class="valueText">{{vendor_name}}</span>
                    </td>
                </tr>

            </table>


        </section>
        <section>
            <table class="tableForData" cell-padding="0">
                <thead>
                    <tr>
                        <th>Sl No.</th>
                        <th>Item Name</th>
                        <th>Item Code</th>
                        <th>unit</th>
                        <th>Qty</th>
                        {{!-- <th>Unit Price</th>
                        <th>Ext. Price</th>
                        <th>Disc. %</th>
                        <th>Disc. Amt</th>
                        <th>Net Ext. Amt.</th>
                        <th>Tax Amt.</th>
                        <th>Total Amt. </th> --}}

                    </tr>
                </thead>
                <tbody>

                    {{#each detailList}}
                    <tr>
                        <td>{{inc @index}}</td>
                        <td>{{item_description}}</td>
                        <td>{{item_code}}</td>
                        <td>{{uom_description}}</td>
                        <td>{{total_quantity}}</td>
                        {{!-- <td>{{unit_price}}</td>
                        <td>{{extended_price}}</td>
                        <td>{{sub_discount_percentage}}</td>
                        <td>{{sub_discount_amount}}</td>
                        <td>{{net_extended_cost}}</td>
                        <td>{{tax_amount}}</td>
                        <td>{{total_amount}}</td> --}}

                    </tr>
                    {{/each }}
                    {{!-- <tr class="footerTotalDown">
                        <td colspan="11">Total Net Amount</td>
                        <td>{{net_payable}}</td>
                    </tr> --}}
                </tbody>
            </table>
        </section>
        {{!-- <footer>{{{loadPage 'doctor_signature_footer.hbs' header}}}</footer> --}}
    </div>
</body>