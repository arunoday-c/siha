{{{importStyle 'css/creditinvoice.css'}}}

<body>
    <div class="print-body">
        {{#hasElement employees}}


        <section>



            <table class="tableForData" cell-padding="0">
                <thead>
                    <tr>
                        <th>Sl No.</th>
                        <th>Employee Code</th>
                        <th>Employee Name</th>
                        <th>Desgnation</th>
                        <th>Nationality</th>
                        {{!-- <th>Dep.</th> --}}
                        {{!-- <th>Working Days</th> --}}
                        <th>Present Days</th>
                        {{#hasElement earning_component}}
                        {{#each earning_component}}
                        <th>{{earning_deduction_description}}</th>
                        {{/each }}
                        {{/hasElement}}
                        <th>Total Earnings</th>
                        {{#hasElement deduction_component}}
                        {{#each deduction_component}}
                        <th>{{earning_deduction_description}}</th>
                        {{/each }}
                        {{/hasElement}}
                        <th>Total Deduction </th>
                        {{#hasElement contributions_component}}
                        {{#each contributions_component}}
                        <th>{{earning_deduction_description}}</th>
                        {{/each }}
                        {{/hasElement}}<th>Net Salary</th>
                        <th>Total Contribution</th>
                        <th>Overtime</th>
                        <th>Employee + Employer</th>
                        <th>Leave Provision</th>
                        <th>Airfrare Amount</th>
                        <th>Gratuity Provision</th>
                        <th>
                            Mode Of Payment
                        </th>
                        <th>
                            Salary Payed Date
                        </th>
                    </tr>
                </thead>
                <tbody>

                    {{#each employees}}
                    <tr>
                        <td>{{inc @index}}</td>
                        <td>{{employee_code}}</td>
                        <td>{{full_name}}</td>
                        <td>{{designation}}</td>
                        <td>{{nationality}}</td>
                        {{!-- <td>{{sub_department_name}}</td> --}}
                        {{!-- <td>{{total_days}}</td> --}}
                        <td>{{display_present_days}}</td>
                        {{#hasElement ../earning_component}}
                        {{#hasElement employee_earning}}
                        {{#each ../earning_component}}
                        <td class="numberFld"> {{dynamicSalary hims_d_earning_deduction_id ../employee_earning "E"}}
                        </td>
                        {{/each }}
                        {{else}}
                        <td>NA</td>
                        {{/hasElement }}
                        {{/hasElement }}
                        <td class="numberFld">{{total_earnings}}</td>
                        {{#hasElement ../deduction_component}}
                        {{#hasElement employee_deduction}}
                        {{#each ../deduction_component}}
                        <td class="numberFld"> {{dynamicSalary hims_d_earning_deduction_id ../employee_deduction "D"}}
                        </td>
                        {{/each }}
                        {{else}}
                        <td>NA</td>
                        {{/hasElement }}
                        {{/hasElement }}
                        <td class="numberFld">{{total_deductions}}</td>
                        {{#hasElement ../contributions_component}}
                        {{#hasElement employee_contributions}}
                        {{#each ../contributions_component}}
                        <td class="numberFld">
                            {{dynamicSalary hims_d_earning_deduction_id ../employee_contributions "C"}}</td>
                        {{/each }}
                        {{else}}
                        <td>NA</td>
                        {{/hasElement }}
                        {{/hasElement }}

                        <td class="numberFld">{{net_salary}}</td>
                        <td class="numberFld">{{total_contributions}}</td>
                        <td>{{complete_ot}}</td>
                        <td>{{employe_plus_employr}}</td>
                        <td class="numberFld">{{leave_salary}}</td>
                        <td class="numberFld">{{airfare_amount}}</td>
                        <td class="numberFld">{{gratuity_amount}}</td>
                        <td>
                            {{mode_of_payment}}
                        </td>
                        <td>
                            {{salary_paid_date}}
                        </td>
                    </tr>
                    {{/each }}{{else}}
                    <tr>
                        <td class="noResultTextReport">No Records Found</td>
                    </tr>
                </tbody>
            </table>


        </section>

        {{/hasElement }}




        <table class="tableTotal">
            <tr>
                <td>
                    Grand Total Earnings
                </td>
                <td>
                    {{sum_earnings}}
                </td>
            </tr>
            <tr>
                <td>
                    Grand Total Deduction
                </td>
                <td>
                    {{sum_deductions}}
                </td>
            </tr>
            <tr>
                <td>
                    Grand Total Contribution
                </td>
                <td>
                    {{sum_contributions}}
                </td>
            </tr>
            <tr>
                <td>
                    Grand Net Salary
                </td>
                <td>
                    {{sum_net_salary}}
                </td>
            </tr>
        </table>
    </div>

</body>