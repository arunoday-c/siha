{{{importStyle 'css/algaehReportStyles.css'}}}

<body>
    <div class="print-body">

        <section>
            <table class="tableForData" cell-padding="0">
                <thead>
                    <tr>
                        <th>Sl No.</th>
                        <th>Employee Code</th>
                        <th>Employee Name</th>
                        <th>ID Type</th>
                        <th>ID No.</th>
                        <th>Username</th>
                        <th>Type</th>
                        <th>Group</th>
                        <th>Role</th>
                        <th>Status</th>
                </thead>
                <tbody>
                {{#each result}}
                    <tr>
                        <td>{{inc @index}}</td>
                        <td>{{employee_code}}</td>
                        <td class="txtFld">{{full_name}}</td>
                        <td>{{identity_document_name}}</td>
                        <td>{{identity_no}}</td>
                        <td>{{username}}</td>
                        <td>{{user_type}}</td>
                        <td>{{app_group_name}}</td>
                        <td>{{role_name}}</td>
                        <td>{{user_status}}</td>
                    </tr>
                {{/each }}
            </tbody>
        </table>
    </section>
        {{!-- <footer>{{{loadPage 'doctor_signature_footer.hbs' header}}}</footer> --}}
    </div>
                   
</body>