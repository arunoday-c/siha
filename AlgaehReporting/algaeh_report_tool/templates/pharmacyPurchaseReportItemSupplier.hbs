<body>
    <div class="print-body">

        <section>
            <table class="tableForData" cell-padding="0">
                <thead>
                    <tr>
                        <th>Sl No.</th>
                        <th>Item Code</th>
                        <th>Item Name</th>
                        <th>Vendor Code</th>
                        <th>Vendor Name</th>
                        <th>Vendor Batch No.</th>
                        <th>PO No.</th>
                        <th>PO Date</th>
                        <th>Delivery No.</th>
                        <th>Delivery Date</th>
                        <th>PO Qty</th>
                        <th>Received Qty</th>
                        <th>Free Qty</th>
                        <th>Unit Cost</th>
                        <th>Sales Price</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each result}}
                    <tr>
                        <td>{{inc @index}}</td>
                        <td>{{item_code}}</td>
                        <td>{{item_description}}</td>
                        <td>{{vendor_code}}</td>
                        <td>{{vendor_name}}</td>
                        <td>{{vendor_batchno}}</td>
                        <td>{{purchase_number}}</td>
                        <td>{{po_date}}</td>
                        <td>{{delivery_note_number}}</td>
                        <td>{{dn_date}}</td>
                        <td>{{po_quantity}}</td>
                        <td>{{dn_quantity}}</td>
                        <td>{{free_qty}}</td>
                        <td>{{unit_cost}}</td>
                        <td>{{sales_price}}</td>
                    </tr>
                    {{/each }}
                </tbody>
            </table>


