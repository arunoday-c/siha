{{{importStyle "css/algaehReportStyles.css"}}}

<body>
  <div class="print-body">
    <section>
      <table class="tableForLabel" cell-padding="0">
        <tr>
          <td><label class="textEnLft">Total No Employees</label>:<span
              class="valueText"
            >{{no_employees}}</span>
          </td>
          <td><label class="textEnLft">Age</label>:<span
              class="valueText"
            >{{return_age}}</span></td>
        </tr>
      </table>
    </section>

    <section>
      <table class="tableForData" cell-padding="0">
        <thead>
          <tr>
            <th>Sl No.</th>
            <th>Primary ID</th>
            <th>Employee Code</th>
            <th>Employee Name</th>
            <th>Age</th>
            <th>DOB</th>
            <th>Gender</th>
            <th>Emp. Group</th>
            <th>Dept. </th>
            <th>Sub Dept.</th>
            <th>Designation</th>
            <th>Date of Joining</th>
            <th>Nationality</th>
            <th>Religion</th>
            <th>Emp. Type</th>
            <th>Emp. Status</th>

          </tr>
        </thead>
        <tbody>
          {{#hasElement result}}
            <tr class="numberFldTotalBottom">
              <td class="txtFld" colspan="11">Total Employee:
                {{no_employees}}</td>
              <td class="numberFld" colspan="5">Age: {{return_age}}</td>
            </tr>
            {{#each result}}
              <tr>
                <td datatype="string">{{inc @index}}</td>
                <td datatype="string">{{identity_no}}</td>
                <td datatype="string">{{employee_code}}</td>
                <td style="text-align: left;">{{full_name}}</td>
                <td>{{new_age}}</td>
                <td datatype="string">{{date_of_birth}}</td>
                <td>{{sex}}</td>
                <td>{{group_description}}</td>
                <td>{{department_name}}</td>
                <td>{{sub_department_name}}</td>
                <td>{{designation}}</td>
                <td>{{date_of_joining}}</td>
                <td>{{nationality}}</td>
                <td>{{religion_name}}</td>
                <td>{{employee_type}}</td>
                <td>{{employee_status}}</td>
              </tr>
            {{/each}}
          {{else}}
            <tr>
              <td colspan="16" class="noResultTextReport">No Records Found</td>
            </tr>{{/hasElement}}
        </tbody>

      </table>
    </section>
    {{!-- <footer>{{{loadPage 'doctor_signature_footer.hbs' header}}}</footer> --}}

  </div>
</body>