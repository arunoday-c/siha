        <section>
            <table class="tableForData" cell-padding="0">
                <thead>

                    <tr>
                        <th>Sl No.</th>
                        <th>Employee Code</th>
                        <th>Employee Name</th>
                        <th>Gender</th>
                        <th>Emp. Group</th>
                        <th>Dept. </th>
                        <th>Sub Dept.</th>
                        <th>Designation</th>
                        <th>Date of Joining</th>
                        <th>Religion</th>
                        <th>Emp. Type</th>
                        <th>Emp. Status</th>

                    </tr>

                </thead>
                <tbody>
                    {{#hasElement result}}

                    {{#each result}}
                    <tr>
                        <td colspan="12"
                            excelfill='{"type":"pattern","pattern":"solid","fgColor":{"argb":"CDF4EF"},"bgColor":{"argb":"747474"}}'
                            class="tableSubHdg">Nationality: {{nationality}}
                            <span class="textFloatRight">No
                                Employees: {{no_employee}}</span>

                        </td>
                    </tr>
                    {{#each employees}}
                    <tr>
                        <td>{{inc @index}}</td>
                        <td>{{employee_code}}</td>
                        <td style="text-align: left;">{{full_name}}</td>
                        <td>{{sex}}</td>
                        <td>{{group_description}}</td>
                        <td>{{department_name}}</td>
                        <td>{{sub_department_name}}</td>
                        <td>{{designation}}</td>
                        <td>{{date_of_joining}}</td>
                        <td>{{religion_name}}</td>
                        <td>{{employee_type}}</td>
                        <td>{{asemployee_status}}</td>
                    </tr>
                    {{/each }}{{/each }}
                    {{else}}
                    <tr>
                        <td colspan="12" class="noResultTextReport">No Records Found</td>
                    </tr>{{/hasElement }}
                </tbody>

            </table>
        </section>
        {{!-- <footer>{{{loadPage 'doctor_signature_footer.hbs' header}}}</footer> --}}

    </div>
</body>