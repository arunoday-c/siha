{{{importStyle 'css/algaehReportStyles.css'}}}

<body>
    <div class="print-body">

        {{!-- <section>
            <table class="tableForLabel" cell-padding="0">
                <tr>
                    <td><label class="textEnLft">Branch</label>:<span class="valueText">{{hospital_name}}</span></td>
                    <td><label class="textEnLft">No Employees</label>:<span class="valueText">{{no_employees}}</span>
                    </td>
                </tr>
            </table>


        </section> --}}
        <section>
            <table class="tableForData" cell-padding="0">
                <thead>
                    <tr>
                        <th>Sl No.</th>
                        <th>Category</th>
                        {{!-- <th>Tax Percentage</th> --}}
                        <th>Tax Amount</th>
                    </tr>
                </thead>
                <tbody>
                    {{#hasElement result}}

                    {{#each result}}
                    <tr>
                        <td colspan="4" class="tableSubHdg txtFld">Slab - {{tax_percentage}}%</td>
                    </tr>
                    {{!-- {{#each item_details}} --}}
                        {{#each catSum}} 
                            <tr>
                            <td>{{inc @index}}</td>
                            <td>{{category_desc}}</th>
                            {{!-- <td class="numberFld">{{tax_percentage}}</td> --}}
                            <td class="numberFld">{{sum_tax_amount}}</td>
                            </tr>
                        {{/each}}
                    {{!-- {{/each }} --}}
                    <tr class="tableSubHdg2">
                        <td colspan="2"></td>
                        {{!-- <td class="numberFld"><b></b></td> --}}
                        <td class="numberFld"><b>{{numberFormating net_sum_tax_amount ../currencyOnly}}</b></td>
                    </tr>
                    {{/each }}
                     <tr class="numberFldTotalBottom">
                        <td colspan="2"></td>
                        {{!-- <td class="numberFld"><b></b></td> --}}

                        <td class="numberFld"><b>{{numberFormating total_net_sum_tax_amount ./currencyOnly}}</b></td>
                    </tr>
                    
                    {{else}}
                    <tr>
                        <td colspan="4" class="noResultTextReport">No Records Found</td>
                    </tr>{{/hasElement }}
                </tbody>

            </table>
        </section>
        {{!-- <footer>{{{loadPage 'doctor_signature_footer.hbs' header}}}</footer> --}}

    </div>
</body>