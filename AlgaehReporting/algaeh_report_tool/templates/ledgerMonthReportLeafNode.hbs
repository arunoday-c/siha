{{{importStyle 'css/algaehReportStyles.css'}}}

<body>
    <div class="print-body">

        <section>
            <table class="tableForData" cell-padding="0">
                <thead>
                    <tr>
                        <th>Sl No.</th>
                        <th>Month</th>
                        <th>Account</th>
                        <th>Debit</th>
                        <th>Credit</th>
                        <th>Balance</th>
                        <th>Narration</th>
                    </tr>
                </thead>
                <tbody>
                    {{#hasElement details}}

                    {{#each details}}
                    <tr>
                        <td>{{inc @index}}</td>
                        <td class="txtFld">{{month}}</td>
                        <td class="numberFld numberFldTotal">{{account_details}}</td>
                        <td class="numberFld numberFldTotal">{{debit_amount}}</td>
                        <td class="numberFld numberFldTotal">{{credit_amount}}</td>
                        <td class="numberFld numberFldTotal">{{balance_amount}}</td>
                        <td class="txtFld">{{narration}}</td>
                    </tr>
                    {{/each }}
                    {{!-- <tr class="numberFld numberFldTotalBottom">
                            <td class="numberFld" colspan="5">Total</td>
                            <td class="numberFld numberFldTotal">{{total_amount}}</td>
                            <td class="numberFld numberFldTotal"></td>
                        </tr>
                        <tr class="numberFld numberFldTotalBottom">
                            <td class="numberFld" colspan="5">Closing Balance</td>
                            <td class="numberFld">{{total_amount}}</td>
                        </tr> --}}
                    {{else}}
                    <tr>
                        <td class="noResultTextReport">No Records Found</td>
                    </tr>
                    {{/hasElement }}
                </tbody>
            </table>


        </section>




        <table class="tableTotal">
            <tr>
                <td>
                    Grand Total Debit
                </td>
                <td>
                    {{000}}
                </td>
            </tr>
            <tr>
                <td>
                    Grand Total Credit
                </td>
                <td>
                    {{00}}
                </td>
            </tr>
            <tr>
                <td>
                    Grand Closing Balance
                </td>
                <td>
                    {{00}}
                </td>
            </tr>

            </tr>
        </table>
    </div>

</body>