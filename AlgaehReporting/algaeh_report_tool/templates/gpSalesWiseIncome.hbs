{{{importStyle "css/algaehReportStyles.css"}}}

<body>
  <div class="print-body">

    <section>
      <table class="tableForData" cell-padding="0">
        <thead>
          <tr>
            <th>Sl No.</th>
            <th>Invoice No.</th>
            <th>Invoice Date</th>
            <th>Customer Name</th>
            <th>Sales Person Name</th>
            <th>Cost</th>
            <th>Weighted Avg Cost.</th>
            <th>Gross Profit</th>
          </tr>
        </thead>
        <tbody>{{#hasElement details}}
            {{#each details}}
              <tr>
                <td datatype="string">{{inc @index}}</td>
                <td datatype="string">{{invoice_number}}</td>
                <td datatype="string">{{invoice_date}}</td>
                <td class="txtFld" datatype="string">{{customer_name}}</td>
                <td class="txtFld" datatype="string">{{sales_per_name}}</td>
                <td class="numberFld">{{net_payable}}</td>
                <td class="numberFld">{{net_payable}}</td>
                <td class="numberFld">{{net_payable}}</td>
              </tr>
            {{/each}}
            <tr class="numberFldTotalBottom">

              <td colspan="7" class="numberFld">Total</td>
              <td class="numberFld">{{net_payable}}</td>
            </tr>
          {{else}}
            <tr>
              <td colspan="8" class="noResultTextReport">No Records Found</td>
            </tr>
          {{/hasElement}}
        </tbody>
      </table>
    </section>
    {{!-- <footer>{{{loadPage 'doctor_signature_footer.hbs' header}}}</footer> --}}
  </div>
</body>