    <section>
      <table class="tableForData" cell-padding="0">
        <thead>
          <tr>
            <tr>
              <th>Sl No.</th>
              <th>Primary ID</th>
              <th>Employee Code</th>
              <th>Employee Name</th>
              <th>Gender</th>
              <th>Employee Group</th>
              <th>Dept. </th>
              <th>Sub Dept.</th>
              <th>Designation</th>
              <th>Date of Joining</th>
              <th>Nationality</th>
              <th>Religion</th>
              <th>Emp. Type</th>
              <th>Emp. Status</th>

            </tr>
          </tr>
        </thead>
        <tbody>
          {{#hasElement result}}

            {{#each result}}
              <tr>
                <td datatype="string">{{inc @index}}</td>
                <td datatype="string">{{identity_no}}</td>
                <td datatype="string">{{employee_code}}</td>
                <td style="text-align: left;">{{full_name}}</td>
                <td>{{sex}}</td>
                <td>{{group_description}}</td>
                <td>{{department_name}}</td>
                <td>{{sub_department_name}}</td>
                <td>{{designation}}</td>
                <td datatype="string">{{date_of_joining}}</td>
                <td>{{nationality}}</td>
                <td>{{religion_name}}</td>
                <td>{{employee_type}}</td>
                <td>{{asemployee_status}}</td>
              </tr>
            {{/each}}
          {{else}}
            <tr>
              <td colspan="14" class="noResultTextReport">No Records Found</td>
            </tr>{{/hasElement}}
        </tbody>

      </table>
    </section>
    {{!-- <footer>{{{loadPage 'doctor_signature_footer.hbs' header}}}</footer> --}}

  </div>
</body>