{{{importStyle "css/algaehReportStyles.css"}}}

<body>
  <div class="print-body">

    {{!-- <section>
            <table class="tableForLabel" cell-padding="0">
                <tr>
                    <td><label class="textEnLft">Branch</label>:<span class="valueText">{{hospital_name}}</span></td>
                    <td><label class="textEnLft">No Employees</label>:<span
                            class="valueText">{{no_employees}}</span>/td>
                    <td><label class="textEnLft">Date Of Joining</label>:<span class="valueText">{{return_input}}</span>
                    </td>
                </tr>
            </table>


