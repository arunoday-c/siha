{
  "name": "algaeh-front-desk",
  "version": "1.0.0",
  "description": "Algaeh Front Desk Module",
  "main": "dist",
  "scripts": {
    "front_desk_server": "cross-env  PORT=3001 NODE_ENV=development nodemon -w src --exec \"babel-node src/front_desk_server.js \"",
    "build": "babel src -s -D -d dist ",
    "dev": "npm run front_desk_server",
    "api": "npm run front_desk_server",
    "lint": "eslint src && eslint test",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env PORT=3001 NODE_ENV=production pm2 start  -i 1 front_desk_server.js"
  },
  "dependencies": {
    "algaeh-mysql": "^1.2.4",
    "body-parser": "^1.18.2",
    "compression": "^1.7.3",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.4",
    "express": "^4.16.3",
    "winston": "^2.4.2",
    "winston-daily-rotate-file": "^3.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.19.1",
    "nodemon": "^1.17.3"
  },
