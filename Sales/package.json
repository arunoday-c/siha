{
  "name": "algaeh-sales",
  "version": "1.0.0",
  "description": "Algaeh Sales",
  "main": "index.js",
  "scripts": {
    "sales_server": "run-p run-webpack run-dev",
    "build": "webpack --config webpack.prod.js",
    "run-webpack": "webpack --watch --config webpack.dev.js",
    "dev": "npm run sales_server",
    "api": "npm run sales_server",
    "lint": "eslint src && eslint test",
    "test": "echo \"Error: no test specified\" && exit 1",
    "run-prod": "cross-env PORT=3021 NODE_ENV=production pm2 start  -i 2 build/app.js --name 'sales'",
    "run-docker": "cross-env PORT=3021 NODE_ENV=production node build/app.js",
    "start": "run-s build run-prod",
    "run-dev": "cross-env PORT=3021 NODE_ENV=development nodemon --exec 'node build/app.js'"
  },
