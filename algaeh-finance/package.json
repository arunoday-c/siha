{
  "name": "algaeh-finance",
  "version": "2.0.0",
  "description": "Algaeh Finance Microservice with frontend",
  "main": "index.js",
  "scripts": {
    "finance_server": "run-p run-webpack run-dev",
    "dev:client": "npm start --prefix client",
    "dev:server": "npm run finance_server",
    "dev": "run-p dev:*",
    "build": "run-p build:*",
    "build:client": "npm run build --prefix client",
    "build:server": "webpack --config webpack.prod.js",
    "start": "cross-env NODE_ENV=production PORT=3007 pm2 start -i 2 build/app.js --name 'finance'",
    "run-docker": "cross-env PORT=3007 NODE_ENV=production node build/app.js",
    "run-webpack": "webpack --watch --config webpack.dev.js",
    "run-dev": "cross-env PORT=3007 NODE_ENV=development nodemon --exec 'node build/app.js'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "algaeh-mysql": "^1.3.4",
    "algaeh-react-components": "^1.2.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.3",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.4",
    "cryptr": "^4.0.0",
    "express": "^4.16.3",
    "extend": "^3.0.2",
    "mathjs": "^5.1.2",
    "node-linq": "0.0.5",
    "semantic-ui-react": "^0.88.1",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "babel-loader": "^8.0.6",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.5",
    "obfuscator-loader": "^1.1.2",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
