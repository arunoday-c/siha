{
  "name": "algaeh-hrmanagement",
  "version": "2.0.0",
  "description": "Algaeh HR Management services",
  "main": "build",
  "scripts": {
    "hr_server": "npm-run-all -p run-webpack run-dev",
    "build": "webpack --config webpack.prod.js",
    "run-webpack": "webpack --watch --config webpack.dev.js",
    "dev": "npm run hr_server",
    "api": "npm run hr_server",
    "lint": "eslint src && eslint test",
    "test": "echo \"Error: no test specified\" && exit 1",
    "run-prod": "cross-env PORT=3004 NODE_ENV=production pm2 start  -i 2 build/app.js --name 'hr_server'",
    "start": "run-s build run-prod",
    "run-dev": "cross-env PORT=3004 NODE_ENV=development nodemon --exec 'node build/app.js'"
  },
  "dependencies": {
    "algaeh-mysql": "^1.3.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.3",
    "concurrently": "^3.5.1",
    "core-js": "^3.2.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.4",
    "cryptr": "^4.0.0",
    "exceljs": "^1.15.0",
    "express": "^4.16.3",
    "extend": "^3.0.2",
    "global": "^4.4.0",
    "mathjs": "^5.1.2",
    "moment": "^2.22.1",
    "mssql": "^4.3.2",
    "node-linq": "0.0.5",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/preset-env": "^7.6.2",
    "babel-loader": "^8.0.6",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.5",
    "obfuscator-loader": "^1.1.2",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "keywords": [],
