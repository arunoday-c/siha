{
  "root": true,
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaFeatures": {
      "modules": true
    },
    "ecmaVersion": 6
  },
  "rules": {
    // "no-unused-expression": "warn",
    "no-eval": ["error", { "allowIndirect": true }],
    "no-script-url": "off"
  }
}
