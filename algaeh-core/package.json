{
  "name": "hims",
  "version": "0.1.0",
  "description": "Hospital Information Management System",
  "main": "dist",
  "scripts": {
    "internal_server": "run-p run-webpack run-dev",
    "debug": "cross-env PORT=3000 NODE_ENV=development nodemon -w src src/internal_server.js --exec \"babel-node --inspect  src/internal_server.js  \"",
    "build": "webpack --config webpack.prod.js",
    "run-dev": "cross-env PORT=3000 NODE_ENV=development nodemon --exec 'node build/app.js'",
    "run-webpack": "webpack --watch --config webpack.dev.js",
    "build_document_management": "babel DocumentManagement/src -s -D -d DocumentManagement/dist ",
    "hr_server": "npm run dev --prefix HrManagement",
    "doc_mgmt": "npm run document_server --prefix DocumentManagement",
    "api": "concurrently \"npm run internal_server\"",
    "dev": "npm run internal_server",
    "npminstall": "npm install && npm install --prefix client",
    "prod_build": "npm run build --prefix client && npm run build",
    "lint": "eslint src && eslint test",
    "pm2": "concurrently --kill-others  cross-env NODE_ENV=development  pm2 start  --max-memory-restart 850M \"babel-node src/internal_server.js \"  \"npm run client\"",
    "test": "export NODE_ENV=test npm run build && mocha --require babel-core/register --timeout=10000000",
    "run-prod": "cross-env PORT=3000 NODE_ENV=production pm2 start  -i 4 build/app.js --name 'core'",
    "run-docker": "cross-env PORT=3000 NODE_ENV=production node build/app.js",
    "start": "run-s build run-prod",
    "start-proxy": "docker-compose -f '../nginx-conf/docker-compose.debug.yml' up --build -d"
  },
  "author": "ALGAEH Technologies Pvt Ltd",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "algaeh-mysql": "^1.4.1",
    "aws-sdk": "^2.247.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.3",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.4",
    "cryptr": "^4.0.0",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "extend": "^3.0.1",
    "file-base64": "^1.0.0",
    "hijri-date": "^0.2.2",
    "jsonwebtoken": "^8.2.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "lokijs": "^1.5.5",
    "mathjs": "^5.1.2",
    "mime": "^2.3.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.1",
    "morgan": "^1.9.1",
    "mssql": "^4.3.0",
    "multer": "^1.3.0",
    "mysql": "^2.15.0",
    "node-linq": "0.0.5",
    "node-model-validation": "^1.0.1",
    "node-string-pad": "0.0.4",
    "nodemailer": "^6.3.0",
    "nodemailer-express-handlebars": "^3.1.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pm2": "^3.0.2",
    "request": "^2.88.0",
    "rotating-file-stream": "^1.3.6",
    "underscore": "^1.9.1",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.1.4",
    "with-context": "0.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "babel-loader": "^8.0.6",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.5",
    "obfuscator-loader": "^1.1.2",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
