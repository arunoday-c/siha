{
  "name": "hims",
  "version": "0.1.0",
  "description": "Hospital Information Management System",
  "main": "dist",
  "scripts": {
    "internal_server": "cross-env NODE_ENV=development nodemon -w src --exec \"babel-node src/internal_server.js  \"",
    "build": "babel src -s -D -d dist ",
    "build_document_management": "babel DocumentManagement/src -s -D -d DocumentManagement/dist ",
    "client": "npm run start --prefix client",
    "hr_server": "npm run dev --prefix HrManagement",
    "doc_mgmt": "npm run document_server --prefix DocumentManagement",
    "api": "concurrently \"npm run internal_server\"  \"npm run hr_server\"",
    "dev": "concurrently --kill-others  \"npm run internal_server \" \"npm run client\"  \"npm run doc_mgmt\"",
    "start12333": "cross-env NODE_ENV=production pm2 start  -i 4 dist/internal_server.js",
    "npminstall": "npm install && npm install --prefix client",
    "prod_build": "npm run build --prefix client && npm run build",
    "lint": "eslint src && eslint test",
    "pm2": "concurrently --kill-others  cross-env NODE_ENV=development  pm2 start  --max-memory-restart 850M \"babel-node src/internal_server.js \"  \"npm run client\"",
    "test": "export NODE_ENV=test npm run build && mocha --require babel-core/register --timeout=10000000",
    "complete-build": "concurrently \"babel src -s -D -d ../algaeh_erp_live/Core \" \"babel HrManagement/src -s -D -d ../algaeh_erp_live/HrManagement\" \"babel DocumentManagement/src -s -D -d ../algaeh_erp_live/DocumentManagement\"",
    "start-core": "cross-env NODE_ENV=production pm2 start  -i 4 internal_server.js"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-eval": true,
      "no-console": 0,
      "no-unused-vars": 1,
      "no-script-url": 0
    }
  },
  "author": "ALGAEH Technologies Pvt Ltd",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.247.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.3",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.4",
    "cryptr": "^4.0.0",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "extend": "^3.0.1",
    "file-base64": "^1.0.0",
    "hijri-date": "^0.2.2",
    "jsonwebtoken": "^8.2.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "lokijs": "^1.5.5",
    "mathjs": "^5.1.2",
    "mime": "^2.3.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.1",
    "morgan": "^1.9.1",
    "mssql": "^4.3.0",
    "multer": "^1.3.0",
    "mysql": "^2.15.0",
    "node-linq": "0.0.5",
    "node-model-validation": "^1.0.1",
    "node-string-pad": "0.0.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pm2": "^3.0.2",
    "request": "^2.88.0",
    "rotating-file-stream": "^1.3.6",
    "underscore": "^1.9.1",
    "winston": "^2.4.2",
    "winston-daily-rotate-file": "^3.1.4",
    "with-context": "0.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "database-cleaner": "^1.2.0",
    "eslint": "^4.19.1",
    "knex": "^0.14.6",
    "knex-cleaner": "^1.1.4",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.3"
  }
}
