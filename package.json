{
  "name": "hims",
  "version": "0.1.0",
  "description": "Hospital Information Management System",
  "main": "dist",
  "scripts": {
    "server": "cross-env NODE_ENV=development nodemon -w src --exec \"babel-node src/server.js --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "client": "npm run start --prefix client",
    "runapi": "npm run server",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "cross-env NODE_ENV=production pm2 start dist/server.js",
    "npminstall": "npm install && npm install --prefix client",
    "prod_build": "npm run build --prefix client && npm run build",
    "lint": "eslint src && eslint test",
    "test": "export NODE_ENV=test npm run build && mocha --require babel-core/register --timeout=10000000"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-eval": 0,
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "ALGAEH Technologies Pvt Ltd",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.247.1",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.4",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "extend": "^3.0.1",
    "file-base64": "^1.0.0",
    "hijri-date": "^0.2.2",
    "jsonwebtoken": "^8.2.1",
    "jwt-decode": "^2.2.0",
    "lokijs": "^1.5.5",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.1",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "mysql": "^2.15.0",
    "node-linq": "0.0.5",
    "node-model-validation": "^1.0.1",
    "node-string-pad": "0.0.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "rc-calendar": "^9.6.1",
    "rc-time-picker": "^3.3.1",
    "react-number-format": "^3.3.4",
    "rotating-file-stream": "^1.3.6",
    "sweetalert": "^2.1.0",
    "underscore": "^1.9.1",
    "winston": "^2.4.2",
    "winston-daily-rotate-file": "^3.1.4",
    "with-context": "0.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "database-cleaner": "^1.2.0",
    "eslint": "^4.19.1",
    "knex": "^0.14.6",
    "knex-cleaner": "^1.1.4",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.3"
  }
}
