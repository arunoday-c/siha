{
  "name": "algaeh-sockets",
  "version": "1.0.0",
  "description": "Algaeh Sockets IO services",
  "main": "build/app.js",
  "scripts": {
    "ioServer": "npm-run-all -p run-webpack run-dev",
    "build": "webpack --config webpack.prod.js",
    "run-webpack": "webpack --watch --config webpack.dev.js",
    "run-prod": "cross-env PORT=3019 NODE_ENV=production pm2 start -i 2 build/app.js  --name 'sockets'",
    "dev": "npm run ioServer",
    "run-dev": "cross-env PORT=3019 NODE_ENV=development nodemon --exec 'node build/app.js'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "ioredis": "^4.14.1",
    "moment": "^2.24.0",
    "mongoose": "^5.9.6",
    "regenerator-runtime": "^0.13.5",
    "socket.io": "^2.2.0",
    "socket.io-redis": "^5.2.0",
    "socketio-auth": "^0.1.1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-eval": 0,
      "no-console": 0,
      "no-unused-vars": 1,
      "no-script-url": 0
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "babel-loader": "^8.0.6",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.5",
    "obfuscator-loader": "^1.1.2",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
